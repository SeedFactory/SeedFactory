<!DOCTYPE html>
<html>
<head>
  <title>SeedFactory</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<div class="navbar navbar-default navbar-fixed-top">
  <div class="container">
    <div class="navbar-header">
      <%= link_to 'SeedFactory', root_url, class: 'navbar-brand' %>
    </div>
    <ul class="nav navbar-nav">
      <li><%= link_to 'Products', products_url %></li>
      <li><%= link_to 'Brands', brands_url %></li>
      <li><%= link_to 'Categories', categories_url %></li>
      <li class="dropdown">
        <a href="#" class="dropdown-toggle" data-toggle="dropdown">Admin <b class="caret"></b></a>
        <ul class="dropdown-menu">
          <li><%= link_to 'Ingredients', ingredients_url %></li>
          <li><%= link_to 'Item Classes', item_classes_url %></li>
          <li><%= link_to 'Markup Tables', markups_url %></li>
          <li><%= link_to 'Package Elements', package_elements_url %></li>
        </ul>
      </li>
    </ul>
  </div>
</div>
<div class="container">
<%= yield %>
</div>
</body>
</html>
