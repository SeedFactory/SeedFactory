<h1>Listing package_element_types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @package_element_types.each do |package_element_type| %>
      <tr>
        <td><%= package_element_type.name %></td>
        <td><%= link_to 'Show', package_element_type %></td>
        <td><%= link_to 'Edit', edit_package_element_type_path(package_element_type) %></td>
        <td><%= link_to 'Destroy', package_element_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Package element type', new_package_element_type_path %>
