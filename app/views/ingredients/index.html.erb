<h1>Listing ingredients</h1>

<table>
  <thead>
    <tr>
      <th>Number</th>
      <th>Description</th>
      <th>Cwt</th>
      <th>Cost</th>
      <th>Protein</th>
      <th>Fat</th>
      <th>Fiber</th>
      <th>Ash</th>
      <th>Calcium</th>
      <th>Phosphorus</th>
      <th>Vitamin a</th>
      <th>Vitamin d3</th>
      <th>Vitamin e</th>
      <th>Vitamin k</th>
      <th>Vitamin b12</th>
      <th>Riboflavin</th>
      <th>Met energy</th>
      <th>Panto</th>
      <th>Niacin</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ingredients.each do |ingredient| %>
      <tr>
        <td><%= ingredient.number %></td>
        <td><%= ingredient.description %></td>
        <td><%= ingredient.cwt %></td>
        <td><%= ingredient.cost %></td>
        <td><%= ingredient.protein %></td>
        <td><%= ingredient.fat %></td>
        <td><%= ingredient.fiber %></td>
        <td><%= ingredient.ash %></td>
        <td><%= ingredient.calcium %></td>
        <td><%= ingredient.phosphorus %></td>
        <td><%= ingredient.vitamin_a %></td>
        <td><%= ingredient.vitamin_d3 %></td>
        <td><%= ingredient.vitamin_e %></td>
        <td><%= ingredient.vitamin_k %></td>
        <td><%= ingredient.vitamin_b12 %></td>
        <td><%= ingredient.riboflavin %></td>
        <td><%= ingredient.met_energy %></td>
        <td><%= ingredient.panto %></td>
        <td><%= ingredient.niacin %></td>
        <td><%= link_to 'Show', ingredient %></td>
        <td><%= link_to 'Edit', edit_ingredient_path(ingredient) %></td>
        <td><%= link_to 'Destroy', ingredient, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ingredient', new_ingredient_path %>
