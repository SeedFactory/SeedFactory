<h1>Listing package_elements</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Package element type</th>
      <th>Number</th>
      <th>Size</th>
      <th>Cost</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @package_elements.each do |package_element| %>
      <tr>
        <td><%= package_element.name %></td>
        <td><%= package_element.package_element_type %></td>
        <td><%= package_element.number %></td>
        <td><%= package_element.size %></td>
        <td><%= package_element.cost %></td>
        <td><%= link_to 'Show', package_element %></td>
        <td><%= link_to 'Edit', edit_package_element_path(package_element) %></td>
        <td><%= link_to 'Destroy', package_element, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Package element', new_package_element_path %>
